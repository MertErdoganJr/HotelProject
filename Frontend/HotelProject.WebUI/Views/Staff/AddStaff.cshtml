@model AddStaffViewModel
@{
    ViewData["Title"] = "AddStaff";
    Layout = "~/Views/AdminLayout/_AdminLayout.cshtml";
}

<form method="post">
    <label>Staff Name-Surname</label>
    <input type="text" asp-for="NameSurname" class="form-control" />
    <br />
    <label>Staff Title</label>
    <input type="text" asp-for="Title" class="form-control" />
    <br />
    <label>Staff Image</label>
    <input type="text" asp-for="Image" class="form-control" />
    <br />
    <label>Social Media 1</label>
    <input type="text" asp-for="SocialMedia1" class="form-control" />
    <br />
    <label>Social Media 2</label>
    <input type="text" asp-for="SocialMedia2" class="form-control" />
    <br />
    <label>Social Media 3</label>
    <input type="text" asp-for="SocialMedia3" class="form-control" />
    <br />
    <button class="btn btn-info">Add Staff</button>
</form>


