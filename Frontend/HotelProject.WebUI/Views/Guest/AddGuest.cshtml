@model CreateGuestDto

@{
    ViewData["Title"] = "AddGuest";
    Layout = "~/Views/AdminLayout/_AdminLayout.cshtml";
}

<form method="post">
    <label>Name</label>
    <input type="text" asp-for="Name" class="form-control" />
    <br />
    <label>Surname</label>
    <input type="text" asp-for="Surname" class="form-control" />
    <br />
    <label>City</label>
    <input type="text" asp-for="City" required class="form-control" />
    <br />
    <button class="btn btn-info">Add Guest</button>
</form>

